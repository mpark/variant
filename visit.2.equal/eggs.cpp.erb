#include <eggs/variant.hpp>

template <int>
struct S {};

int main() {
  eggs::variant<<%= (0...n).map { |i| "S<#{i}>" }.join(', ') %>>
      v(S<0>{}),
      w(S<<%= n - 1 %>>{});
#if defined(METABENCH)
  eggs::apply([](auto, auto) {}, v, w);
  eggs::apply([](auto, auto) {}, w, v);
#endif
}
